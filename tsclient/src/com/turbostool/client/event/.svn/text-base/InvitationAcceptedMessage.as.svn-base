package com.turbostool.client.event
{
public class InvitationAcceptedMessage
{
    public static const INVITATION_ACCEPTED: String = "accepted";

    private var _raceID: int;

    public function InvitationAcceptedMessage(raceID: int)
    {
        this._raceID = raceID;
    }

    public function get raceID():int
    {
        return _raceID;
    }

    public function set raceID(value:int):void
    {
        _raceID = value;
    }
}
}