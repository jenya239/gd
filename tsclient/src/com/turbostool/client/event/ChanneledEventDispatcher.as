package com.turbostool.client.event
{

import flash.events.EventDispatcher;

public class ChanneledEventDispatcher extends EventDispatcher
{
    private var _channel: String;

    public function ChanneledEventDispatcher(channel: String)
    {
        super();
        _channel = channel;
    }

    public function get channel():String
    {
        return _channel;
    }
}
}