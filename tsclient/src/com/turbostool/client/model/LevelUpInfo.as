package com.turbostool.client.model
{
import mx.collections.ArrayCollection;

[Bindable]
public class LevelUpInfo
{
    private var _message: String;
    private var _newUpgrades: Array;
    private var _newRoutes: Array;
    private var _newCars: ArrayCollection;

    public function get message():String {
        return _message;
    }

    public function set message(value:String):void {
        _message = value;
    }

    public function get newUpgrades():Array {
        return _newUpgrades;
    }

    public function set newUpgrades(value:Array):void {
        _newUpgrades = value;
    }

    public function get newRoutes():Array {
        return _newRoutes;
    }

    public function set newRoutes(value:Array):void {
        _newRoutes = value;
    }

    public function get newCars():ArrayCollection {
        return _newCars;
    }

    public function set newCars(value:ArrayCollection):void {
        _newCars = value;
    }
}
}