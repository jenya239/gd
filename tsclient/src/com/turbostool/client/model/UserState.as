package com.turbostool.client.model
{
import flash.events.EventDispatcher;
[Bindable]
public class UserState extends EventDispatcher
{
    private var _name: String;
    private var _userTransferState: UserTransferState;
    private var _currentCity: Number;

    public function UserState() {
    }

    public function get name():String
    {
        return _name;
    }

    public function set name(value:String):void
    {
        _name = value;
    }

    public function get userTransferState():UserTransferState
    {
        return _userTransferState;
    }

    public function set userTransferState(value:UserTransferState):void
    {
        _userTransferState = value;
    }

    public function get currentCity():Number
    {
        return _currentCity;
    }

    public function set currentCity(value:Number):void
    {
        _currentCity = value;
    }
}
}