    private var _raceID: int;
    private var _routeName: String;
    private var _fromClientID: int;

    public function get raceID():int
    {
        return _raceID;
    }

    public function set raceID(val:int):void
    {
        _raceID = val;
    }

    public function get routeName():String
    {
        return _routeName;
    }

