package com.turbostool.client.utils.collections
{
public class ArrayList2 extends ArrayList
{
    public function ArrayList2()
    {

    }

    public override function getItemIndex(item: Object):Number
    {
        for (var i: uint = 0; i < items.length; i++)
        {
            if (items[ i ].equals(item))
                return i;
        }
        return -1;
    }
}
}