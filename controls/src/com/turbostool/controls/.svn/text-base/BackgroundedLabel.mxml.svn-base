<?xml version="1.0" ?>
<controls:CustomToolTip
        xmlns:mx="http://www.adobe.com/2006/mxml"
        xmlns:controls="com.turbostool.controls.*"
        backgroundColor="#DE8A04" backgroundAlpha="0.0"
        >
    <mx:Script><![CDATA[
        [Bindable]
        public var labelText:String;

        [Bindable]
        public var backgroundColor2:uint;

        [Bindable]
        public var borderColor2: uint;

        [Bindable]
        public var borderWidth: uint = 0;

        [Bindable]
        public var backgroundAlpha2: Number = 0.7;

        ]]></mx:Script>

    <controls:Rectangle backgroundColor2="{backgroundColor2}" backgroundAlpha="{backgroundAlpha2}" borderColor2="{borderColor2}" borderWidth="{borderWidth}" cornerRadius="6" left="0" right="0" top="0"
                        bottom="0"/>
    <mx:Text htmlText="{labelText}" horizontalCenter="0" verticalCenter="0" textAlign="center" fontFamily="Arial"
              fontSize="11" fontWeight="bold" color="#FFFFFF"/>

</controls:CustomToolTip>